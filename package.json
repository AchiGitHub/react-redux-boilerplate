{
  "name": "react-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.1.0",
    "redux-logic": "^2.1.1",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "npm test -- --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 10,
        "lines": 10,
        "statements": 10
      },
      "./src/components/": {
        "branches": 10,
        "statements": 10
      }
    },
    "coverageReporters": [
      "html"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "node-sass": "^4.13.0",
    "redux-logic-test": "^2.0.0",
    "redux-mock-store": "^1.5.3"
  }
}
